{
  "name": "blockapps-js",
  "version": "3.1.1",
  "description": "Interface to the Ethereum blockchain providing contract creation and transactions, state inspection, and dApp registration and management",
  "keywords": [
    "ethereum",
    "blockchain",
    "blockapps",
    "dapp"
  ],
  "main": "index.js",
  "homepage": "https://github.com/blockapps/blockapps-js",
  "bugs": {
    "url": "https://github.com/blockapps/blockapps-js/issues",
    "email": "ryan@blockapps.net"
  },
  "license": "MIT",
  "author": {
    "name": "Ryan Reich",
    "email": "ryan@blockapps.net",
    "url": "https://github.com/ryanreich"
  },
  "contributors": [
    "BlockApps team (http://blockapps.net)",
    "Prashant Pawar <prashantpawar@gmail.com>"
  ],
  "files": [
    "js/",
    "dist/blockapps.js",
    "dist/blockapps-min.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blockapps/blockapps-js/blockapps-js.git"
  },
  "dependencies": {
    "bignumber.js": "^2.1.4",
    "bluebird": "^2.10.2",
    "bn.js": "^4.11.0",
    "elliptic": "^6.2.3",
    "enum": "^2.3.0",
    "request": "^2.69.0",
    "rlp": "^1.1.2",
    "js-sha3": "^0.5.1",
    "randombytes": "^2.0.2",
    "mnemonic": "^1.0.1"
  },
  "engines": {
    "node": "^5.0.0"
  },
  "devDependencies": {
    "browser-request": "",
    "browserify": "^11.0.1",
    "eslint": "^3.0.1",
    "minify": ""
  },
  "scripts": {
    "build": "npm run browserify && npm run minify",
    "browserify": "./node_modules/browserify/bin/cmd.js -r .:blockapps-js -r bluebird -o dist/blockapps.js",
    "minify": "./node_modules/minify/bin/minify.js dist/blockapps.js >dist/blockapps-min.js",
    "eslint": "./node_modules/eslint/bin/eslint.js -c .eslintrc --ignore-path .gitignore js"
  },
  "browser": {
    "request": "browser-request",
    "./js/routes/readfile.js": "./js/routes/readfile-browser.js"
  },
  "pre-commit": {
    "run": [
      "browserify",
      "minify"
    ]
  }
}
